<h1>New Wine</h1>

<%= render partial: "layouts/errors", locals: {object: @wine} %>

<%= form_for(@wine) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :img %>
  <%= f.text_field :img %>
  <br>
  <br>
  <%= f.label :content %>
  <%= f.text_area :content %>
  <br>
  <br>
  <%= f.submit %>

<% end %>